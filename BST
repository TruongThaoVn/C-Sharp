public class Solution {
    int? val=null;
    
    public bool IsValidBST(TreeNode root) {
    
        
        if (root==null) return true;
        else{
            if (!IsValidBST(root.left)) return false;
            if (val!=null && root.val<=val) return false;
            val=root.val;
            return IsValidBST(root.right);
        }
        
    }
}
